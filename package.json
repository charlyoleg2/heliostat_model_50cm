{
	"name": "heliostat_model_50cm",
	"version": "0.0.1",
	"description": "parameters and stl for a model of heliostat",
	"private": true,
	"type": "module",
	"main": "index.js",
	"scripts": {
		"designix-ui": "designix-ui",
		"designix": "designix",
		"designix-help": "designix --help",
		"base_list-outopt": "designix --design heliostat/base list-outopt",
		"base_json_param": "designix --design=heliostat/base write json_param",
		"base_openscad_long": "designix --design=heliostat/base --param=base/px_base_v01.json --outDir=base --outFileName=base_v01.scad write scad_3d_openscad",
		"base_openscad": "designix -d=heliostat/base -p=base/px_base_v01.json -o=base --outFileName=base_v01.scad write scad_3d_openscad",
		"base_view": "openscad base/base_v01.scad",
		"base_stl": "openscad -o base/base_v01.stl base/base_v01.scad",
		"make_heliostat": "node make_heliostat.js",
		"git_clean": "git clean -f",
		"ls_output": "ls -ltra output"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/charlyoleg2/heliostat_model_50cm.git"
	},
	"keywords": [
		"heliostat",
		"model",
		"designix",
		"parametrix"
	],
	"author": "charlyoleg2",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/charlyoleg2/heliostat_model_50cm/issues"
	},
	"homepage": "https://github.com/charlyoleg2/heliostat_model_50cm#readme",
	"devDependencies": {
		"designix-cli": "^0.5.16",
		"designix-uis": "^0.5.6"
	}
}
